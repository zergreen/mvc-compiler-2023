<!DOCTYPE html>
<html>
<head>
    <title>JSON Data Table</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">

    <style>
        /* Apply the white-space property to preserve line breaks */
        td.output-syntax {
            white-space: pre-line;
        }
    </style>
</head>
<body>
   <div class="container mt-5">
    <table class="table table-striped">
        <thead>
            <tr>
                <th>ID</th>
                <th>Source Code</th>
                <th>Output Syntax</th>
            </tr>
        </thead>
        <tbody>
            <% response.forEach(function(item) { %>
                <tr>
                    <td><%= item.id %></td>
                    <td class="output-syntax"><%= item.src_code %></td>
                    <td class="output-syntax"><%= item.output_syntax %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
   </div>
</body>
</html>
